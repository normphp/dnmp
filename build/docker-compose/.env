# 通用配置
NETWORKS=dnmpNat
# -------------环境模式配置-------------------

# -------------docker-compose-nginx.yml-------------
NGINX_NAME=nginx
NGINX_IMAGE=./nginx
NGINX_PORTS_HTTP=80
NGINX_PORTS_HTTPS=443
NGINX_PORTS_HTTP2=8080
NGINX_NETWORKS=dnmpNat


